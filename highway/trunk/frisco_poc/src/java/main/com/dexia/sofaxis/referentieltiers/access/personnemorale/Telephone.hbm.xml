<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.dexia.sofaxis.referentieltiers.access.personnemorale.Telephone" table="TELEPHONE" lazy="false">
        <id name="telephoneId" type="com.dexia.sofaxis.tools.hibernate.UUIDUserType" access="field">
            <column name="TELEPHONEID" />
            <generator class="assigned" />
        </id>
        <property name="valeur" access="field">
            <column name="VALEUR" />
        </property>
        <property name="description" access="field">
            <column name="DESCRIPTION" />
        </property>
        <property name="typeTelephone" access="field">
            <column name="TYPETELEPHONE" />
        </property>
    </class>
</hibernate-mapping>
