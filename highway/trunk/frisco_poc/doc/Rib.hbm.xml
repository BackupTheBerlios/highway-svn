<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.dexia.sofaxis.referentieltiers.access.rib.Rib" lazy="false" table="RIB" >
        <id name="ribId" type="com.dexia.sofaxis.tools.hibernate.UUIDUserType" access="field">
            <column name="RIBID" />
            <generator class="assigned" />
        </id>
        <property name="numeroCompte" access="field">
            <column name="NUMEROCOMPTE" />
        </property>
        <property name="codeBanque" access="field">
            <column name="CODEBANQUE" />
        </property>
        <property name="codeGuichet" access="field">
            <column name="CODEGUICHET" />
        </property>
        <property name="cle" access="field">
            <column name="CLE" />
        </property>
        <property name="domiciliation"  access="field">
            <column name="DOMICILIATION" />
        </property>
    </class>
</hibernate-mapping>
